#!/usr/bin/env bash

#DDS_NEED_ARRAY

#DDS_USER_OPTIONS

# create working dir
eval JOB_WRK_DIR=%DDS_JOB_ROOT_WRK_DIR%/${LSB_JOBID}_${LSB_JOBINDEX}
mkdir -p $JOB_WRK_DIR
cd $JOB_WRK_DIR

