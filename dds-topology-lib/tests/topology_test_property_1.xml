<topology id="myTopology">

    <var id="numCollections" value="1" />
	<var id="userTask0" value="userTask0.exe" />
	<var id="userTask1" value="userTask1.exe" />

	<property id="property_1" scope="collection" />
	<property id="property_2" scope="collection" />
	<property id="property_3" scope="collection" />
	<property id="property_4" scope="collection" />
	<property id="property_5" scope="collection" />
	<property id="property_6" scope="collection" />
	<property id="property_7" scope="collection" />
	<property id="property_8" scope="collection" />
	<property id="property_9" scope="collection" />
	<property id="property_10" scope="collection" />

  <decltask id="TestKeyValue0">
		<exe reachable="false">${userTask0}</exe>
		<properties>
			<id access="write">property_1</id>
			<id access="write">property_2</id>
			<id access="write">property_3</id>
			<id access="write">property_4</id>
			<id access="write">property_5</id>
			<id access="read">property_6</id>
			<id access="read">property_7</id>
			<id access="read">property_8</id>
			<id access="read">property_9</id>
			<id access="read">property_10</id>
		</properties>
	</decltask>

  	<decltask id="TestKeyValue1">
		<exe reachable="false">${userTask1}</exe>
		<properties>
			<id access="read">property_1</id>
			<id access="read">property_2</id>
			<id access="read">property_3</id>
			<id access="read">property_4</id>
			<id access="read">property_5</id>
			<id access="write">property_6</id>
			<id access="write">property_7</id>
			<id access="write">property_8</id>
			<id access="write">property_9</id>
			<id access="write">property_10</id>
		</properties>
	</decltask>
    
    <declcollection id="TestCollection">
        <tasks>
            <id>TestKeyValue0</id>
            <id>TestKeyValue0</id>
            <id>TestKeyValue0</id>
            <id>TestKeyValue0</id>
            <id>TestKeyValue0</id>
            <id>TestKeyValue1</id>
            <id>TestKeyValue1</id>
            <id>TestKeyValue1</id>
            <id>TestKeyValue1</id>
            <id>TestKeyValue1</id>
        </tasks>
    </declcollection>

	<main id="main">
        <group id="group1" n="${numCollections}">
			<collection>TestCollection</collection>
		</group>
	</main>

</topology>
